<p id="notice"><%= notice %></p>

<h1>Papers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Tag</th>
      <th>Character</th>
      <th>Faction</th>
      <th>Country</th>
      <th>Race</th>
      <th>Family</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @papers.each do |paper| %>
      <tr>
        <td><%= paper.title %></td>
        <td><%= paper.body %></td>
        <td><%= paper.tag %></td>
        <td><%= paper.character_id %></td>
        <td><%= paper.faction_id %></td>
        <td><%= paper.country_id %></td>
        <td><%= paper.race_id %></td>
        <td><%= paper.family_id %></td>
        <td><%= link_to 'Show', paper %></td>
        <td><%= link_to 'Edit', edit_paper_path(paper) %></td>
        <td><%= link_to 'Destroy', paper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paper', new_paper_path %>
